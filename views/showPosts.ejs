<% include ./partials/header.ejs %>

    <div class="w-full min-h-screen bg-zinc-800 px-10 py-5 flex flex-wrap text-white gap-10">

        <% user.posts.forEach(function(post){ %>
            <div class="card w-32">
                <div class="w-full h-40 bg-sky-200 rounded-lg overflow-hidden">
                    <img src="/images/uploads/<%= post.image %>" class="w-full h-full object-cover">
                </div>
                <h5 class="mt-2">
                    <%= post.title %>
                </h5>
                <!-- <h6 class="opacity-60">user</h6> -->
            </div>
            <% }) %>



    </div>


    <% include ./partials/footer.ejs %>